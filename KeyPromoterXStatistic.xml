<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Commit Message History">
        <Item>
          <IdeaActionID>Vcs.ShowMessageHistory</IdeaActionID>
          <ShortCut>'Strg+M' or 'Strg+E'</ShortCut>
          <Description>Commit Message History</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Commit...">
        <Item>
          <IdeaActionID>CheckinProject</IdeaActionID>
          <ShortCut>'Strg+K'</ShortCut>
          <Description>Commit...</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Copy">
        <Item>
          <IdeaActionID>$Copy</IdeaActionID>
          <ShortCut>'Strg+C' or 'Strg+Einfg'</ShortCut>
          <Description>Copy</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="Delete...">
        <Item>
          <IdeaActionID>$Delete</IdeaActionID>
          <ShortCut>'Entf'</ShortCut>
          <Description>Delete...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Find Usages">
        <Item>
          <IdeaActionID>FindUsages</IdeaActionID>
          <ShortCut>'Alt+F7'</ShortCut>
          <Description>Find Usages</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Hide All Windows">
        <Item>
          <IdeaActionID>HideAllWindows</IdeaActionID>
          <ShortCut>'Strg+Umschalt+F12'</ShortCut>
          <Description>Hide All Windows</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Jump to Source">
        <Item>
          <IdeaActionID>EditSource</IdeaActionID>
          <ShortCut>'F4'</ShortCut>
          <Description>Jump to Source</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="New Scratch File">
        <Item>
          <IdeaActionID>NewScratchFile</IdeaActionID>
          <ShortCut>'Strg+Alt+Umschalt+Einfg'</ShortCut>
          <Description>New Scratch File</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'Alt+1'</ShortCut>
          <Description>Project</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Reformat Code">
        <Item>
          <IdeaActionID>ReformatCode</IdeaActionID>
          <ShortCut>'Strg+Alt+L'</ShortCut>
          <Description>Reformat Code</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rename...">
        <Item>
          <IdeaActionID>RenameElement</IdeaActionID>
          <ShortCut>'Umschalt+F6'</ShortCut>
          <Description>Rename...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Revert...">
        <Item>
          <IdeaActionID>ChangesView.Revert</IdeaActionID>
          <ShortCut>'Strg+Alt+Z'</ShortCut>
          <Description>Revert...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Set Active Changelist">
        <Item>
          <IdeaActionID>ChangesView.SetDefault</IdeaActionID>
          <ShortCut>'Strg+Leertaste'</ShortCut>
          <Description>Set Active Changelist</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Strg+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>16</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show Diff">
        <Item>
          <IdeaActionID>Diff.ShowDiff</IdeaActionID>
          <ShortCut>'Strg+D'</ShortCut>
          <Description>Show Diff</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="Structure">
        <Item>
          <IdeaActionID>ActivateStructureToolWindow</IdeaActionID>
          <ShortCut>'Alt+7'</ShortCut>
          <Description>Structure</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'Alt+F12'</ShortCut>
          <Description>Terminal</Description>
          <Count>12</Count>
        </Item>
      </Statistic>
      <Statistic Action="Update Project...">
        <Item>
          <IdeaActionID>Vcs.UpdateProject</IdeaActionID>
          <ShortCut>'Strg+T'</ShortCut>
          <Description>Update Project...</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="Version Control">
        <Item>
          <IdeaActionID>ActivateVersionControlToolWindow</IdeaActionID>
          <ShortCut>'Alt+9' or 'Alt+Umschalt+9'</ShortCut>
          <Description>Version Control</Description>
          <Count>12</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Back">
        <Item>
          <IdeaActionID>Back</IdeaActionID>
          <ShortCut>'Strg+Alt+Links'</ShortCut>
          <Description>Back</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Strg+B'</ShortCut>
          <Description>Declaration</Description>
          <Count>18</Count>
        </Item>
      </Statistic>
      <Statistic Action="Delete Changelist">
        <Item>
          <IdeaActionID>ChangesView.RemoveChangeList</IdeaActionID>
          <Description>Delete Changelist</Description>
        </Item>
      </Statistic>
      <Statistic Action="File Transfer">
        <Item>
          <IdeaActionID>ActivateFileTransferToolWindow</IdeaActionID>
          <Description>File Transfer</Description>
        </Item>
      </Statistic>
    </option>
  </component>
</application>